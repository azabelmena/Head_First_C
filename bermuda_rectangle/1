#include"bermuda_rectangle.h"
#include<stdio.h>
#include<stdlib.h>


int main(){

    struct pos Pos = {0,0};

    printf("We are at %d latitiude, %d longitude arrr!\n", Pos.x, Pos.y);

    Pos = go_north(Pos);
    printf("We are now at %d latitiude, %d longitude arrr!\n", Pos.x, Pos.y);
    printf("%s\n", get_direction(Pos));

    return 0;
}
